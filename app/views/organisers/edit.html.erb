<%= form_for @organiser, :url => organiser_path(@organiser) do |f| %>
  <fieldset>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </fieldset>
  <fieldset>
    <%= f.label :bio %>
    <%= f.text_field :bio %>
  </fieldset>
  <fieldset>
    <%= f.label :image_url %>
    <%= f.text_field :image_url %>
  </fieldset>
  <fieldset class="button">
    <%= f.submit "Save" %>
  </fieldset>
  <% end %>
